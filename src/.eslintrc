{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "linebreak-style": ["off", "windows"],
    "indent": ["error", 4],
    "arrow-parens": "off",
    "require-jsdoc": "off",
    "no-extra-semi": "off",
    "eol-last": "off",
    "operator-linebreak": "off",
    "max-len": ["error", { "code": 120 }]
  }
}